<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:foregroundGravity="center"
    android:visibility="visible">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="70dp"
        android:layout_height="70dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.046"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.024"
        app:srcCompat="@drawable/logo_app" />

    <Button
        android:id="@+id/buttonNuevaCat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#FFFFFF"
        android:fontFamily="@font/fredoka_one"
        android:text="Crear nueva categoría"
        android:textColor="#2C8F30"
        android:textSize="8sp"
        android:translationZ="-1px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.165" />

    <ImageButton
        android:id="@+id/buttonConf"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/configuraciones"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.918"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.079"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="367dp"
        android:layout_height="410dp"
        android:paddingHorizontal="14dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.65999997"
        tools:ignore="SpeakableTextPresentCheck">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="visible">

            <LinearLayout
                android:id="@+id/Ll1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/CAT1"
                    android:layout_width="293dp"
                    android:layout_height="wrap_content"
                    android:backgroundTint="#FFC107"
                    android:text="Button"
                    android:visibility="visible" />

                <ImageButton
                    android:id="@+id/butB1"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:background="@mipmap/cubo_basura"
                    android:visibility="visible"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/Ll2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/CAT2"
                    android:layout_width="293dp"
                    android:layout_height="wrap_content"
                    android:backgroundTint="#DC92AB"
                    android:text="Button"
                    android:visibility="visible" />

                <ImageButton
                    android:id="@+id/butB2"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:background="@mipmap/cubo_basura"
                    android:visibility="visible"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/Ll3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/CAT3"
                    android:layout_width="293dp"
                    android:layout_height="wrap_content"
                    android:backgroundTint="#45BECD"
                    android:text="Button"
                    android:visibility="visible" />

                <ImageButton
                    android:id="@+id/butB3"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:background="@mipmap/cubo_basura"
                    android:visibility="visible"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/Ll4"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/CAT4"
                    android:layout_width="293dp"
                    android:layout_height="wrap_content"
                    android:backgroundTint="#E17452"
                    android:text="Button"
                    android:visibility="visible" />

                <ImageButton
                    android:id="@+id/butB4"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:background="@mipmap/cubo_basura"
                    android:visibility="visible"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/Ll5"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/CAT5"
                    android:layout_width="293dp"
                    android:layout_height="wrap_content"
                    android:backgroundTint="#8BC34A"
                    android:text="Button"
                    android:visibility="visible" />

                <ImageButton
                    android:id="@+id/butB5"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:background="@mipmap/cubo_basura"
                    android:visibility="visible"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/Ll6"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/CAT6"
                    android:layout_width="293dp"
                    android:layout_height="wrap_content"
                    android:backgroundTint="#673AB7"
                    android:text="Button"
                    android:visibility="visible" />

                <ImageButton
                    android:id="@+id/butB6"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:background="@mipmap/cubo_basura"
                    android:visibility="visible"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/Ll7"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/CAT7"
                    android:layout_width="293dp"
                    android:layout_height="wrap_content"
                    android:backgroundTint="#B551C6"
                    android:text="Button"
                    android:visibility="visible" />

                <ImageButton
                    android:id="@+id/butB7"
